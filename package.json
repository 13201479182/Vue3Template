{
  "name": "vue3-base-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "init": "simple-git-hooks",
    "commit": "gitmoji -c",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.cjs"
  },
  "simple-git-hooks": {
    "pre-commit": "git pull && npx lint-staged"
  },
  "lint-staged": {
    "*.{js,cjs}": "eslint "
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.38.0",
    "gitmoji-cli": "^8.0.0",
    "lint-staged": "^13.2.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
